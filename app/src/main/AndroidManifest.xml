<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cuanbo.cb_iot">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Presentation.MeetingListActivity"
            android:label="@string/title_activity_meeting_room_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".View.Presentation.AddAppointmentActivity"
            android:label="@string/title_activity_add_appointment"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>